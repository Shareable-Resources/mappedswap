{
  "name": "decats",
  "version": "1.0.0",
  "description": "decats backend",
  "_main": "index.js",
  "engines": {
    "node": ">=14.18.0"
  },
  "scripts": {
    "cp:public": "cp -r src/public/. dist/public",
    "pm2": "./node_modules/.bin/pm2",
    "prebuild": "npm run build:contract && npm run build:apiDoc",
    "build": "npm run tsc && npm run webpack",
    "postbuild": "npm run cp:public",
    "build:local": "npm run tsc local && npm run webpack local",
    "build:mainnet": "npm run tsc mainnet && npm run webpack mainnet",
    "build:dev": "npm run tsc dev && npm run webpack dev",
    "build:testnet": "npm run tsc testnet && npm run webpack testnet",
    "build:contract": "node_modules/.bin/typechain --target web3-v1 --out-dir src/@types  'src/abi/*.json'",
    "build:apiDoc": "node_modules/.bin/redoc-cli bundle -o src/public/index.html doc/api.json",
    "pretsc": "./node_modules/.bin/tsc -p './src/general'",
    "tsc": "cd script && bash ./0_tscService.sh",
    "webpack": "bash ./1_webpackService.sh",
    "local": "cd script && bash ./2_runLocal.sh",
    "dev": "cd script && bash ./2_runDev.sh",
    "testnet": "cd script && bash ./2_runTestnet.sh",
    "mainnet": "cd script && bash ./2_runMainnet.sh",
    "init:local": "export NODE_ENV=local && node ./dist/tsc/build/general/CreateTable.js",
    "deploy": "cd script && bash ./3_deploy.sh",
    "remoteRun": "cd script && bash ./4_remoteRun.sh",
    "inspect:cronJob": "export NODE_ENV=dev && node --inspect ./dist/tsc/debug/server/cronJob/index.js",
    "inspect:after": "open chrome and then type chrome://inspect"
  },
